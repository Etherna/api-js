{
  "name": "@etherna/sdk-js",
  "version": "1.0.0",
  "description": "Etherna SDKs for operations on the network",
  "author": "Mattia Dalzocchio",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etherna/etherna-sdk-js.git"
  },
  "keywords": [
    "etherna",
    "clients",
    "swarm",
    "bee",
    "index",
    "gateway",
    "sso",
    "schema",
    "manifest",
    "serializer",
    "deserializer"
  ],
  "bugs": {
    "url": "https://etherna.atlassian.net/jira/software/c/projects/ESJ/issues"
  },
  "homepage": "https://github.com/etherna/clients#readme",
  "scripts": {
    "dev": "node bundle.mjs --watch",
    "build": "node bundle.mjs && tsc -p tsconfig.build.json",
    "test": "yarn vitest",
    "test-ui": "yarn vitest --ui",
    "lint": "yarn eslint --ext .ts --fix .",
    "format": "yarn prettier --write ."
  },
  "peerDependencies": {
    "axios": ">=1.4",
    "immer": ">=10.0",
    "zustand": ">=4.3"
  },
  "dependencies": {
    "@fairdatasociety/bmt-js": "2.1.0",
    "@noble/hashes": "1.3.1",
    "@noble/secp256k1": "2.0.0",
    "axios": "1.4.0",
    "blurhash": "2.0.5",
    "cookiejs": "2.1.2",
    "crypto-ts": "1.0.2",
    "immer": "10.0.2",
    "js-sha3": "0.8.0",
    "zod": "3.21.4",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.27.3"
  },
  "optionalDependencies": {
    "@vitest/ui": "0.33.0",
    "chokidar": "3.5.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": []
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}